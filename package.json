{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "test": "vitest run --silent=false",
    "coverage": "vitest run --coverage",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "serve": "node .output/server/index.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "auth:schema": "npx @better-auth/cli generate --config server/utils/auth.ts --output server/database/schema/auth.ts -y",
    "db:generate": "drizzle-kit generate --config ./server/database/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./server/database/drizzle.config.ts"
  },
  "dependencies": {
    "@better-auth/stripe": "^1.2.7",
    "@nuxt/ui": "^3.0.2",
    "@nuxtjs/i18n": "^9.5.3",
    "@vueuse/integrations": "^13.1.0",
    "better-auth": "^1.2.7",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "0.41.0",
    "ioredis": "^5.6.1",
    "nuxt": "3.16.2",
    "nuxt-echarts": "^0.3.0",
    "nuxt-zod-i18n": "^1.11.5",
    "pg": "8.14.1",
    "resend": "^4.2.0",
    "sortablejs": "^1.15.6",
    "stripe": "^18.0.0",
    "vue": "^3.5.13",
    "vue-component-type-helpers": "^2.2.8",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@better-auth/cli": "^1.2.7",
    "@iconify-json/lucide": "^1.2.36",
    "@iconify-json/simple-icons": "^1.2.32",
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/test-utils": "^3.17.2",
    "@types/pg": "^8.11.12",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.1",
    "@vue/test-utils": "^2.4.6",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.24.0",
    "happy-dom": "^17.4.4",
    "playwright-core": "^1.51.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
